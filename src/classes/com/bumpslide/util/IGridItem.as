package com.bumpslide.util {	    /**     * Grid Item Interface     *      * This interface should be implemented by clips that      * are controlled by a GridLayout instance.     *      * @author David Knape     */    public interface IGridItem {    	    	function destroy () : void;    	    	function set gridIndex (n:int):void;    	function get gridIndex () : int;    	    	function set gridItemData (d:*):void;    	function get gridItemData () : *;    	    	    	function set x (n:Number) : void;    	    	function set y (n:Number) : void;    	    	function set visible (val:Boolean) : void;    	function get visible () : Boolean;    	    	    }}