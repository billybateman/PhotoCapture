package com.bumpslide.events {	    import flash.events.Event;        /**     * Generic Bubbling Event to be broadcast from UI Components      *      * - bubbles by default (to send from display objects)     * - contains generic data object     *      * @author David Knape     */    public class UIEvent extends Event {    	    	private var _data:Object;    	        public function UIEvent(type : String, data:Object=null ) {        	super(type, true);        	//trace('[UIEvent] (\' '+type+'\', '+data +' )');        	_data = data;        }                public function get data () : Object {        	return _data;        }                    }}