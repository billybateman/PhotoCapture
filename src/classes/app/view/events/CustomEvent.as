package app.view.events{import flash.events.Event;	public class CustomEvent extends Event	{		public static const TEAM_OVER:String = "onTeamOver";		public static const TEAM_OUT:String = "onTeamOut";		public static const TEAM_CLICK:String = "onTeamClick";		public static const MAP_READY:String = "onMapReady";				public static const LOAD_LEG_DATA:String = "onLoadLegData";		public static const LOAD_LEG_DATA_LATEST:String = "onLoadLegDataLatest";				public var data:*;				public function CustomEvent(type:String, data:*)		{			this.data= data;			super(type);		}	}}