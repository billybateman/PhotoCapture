package app.controller {        import org.puremvc.interfaces.ICommand;    import org.puremvc.interfaces.INotification;    import app.model.StageProxy;            /**     * Triggers Stage resize notification to update views      * that have been created since the last stage resize.     *      * Thanks to Adrian for pointing out that this logic      * should be in a command :) http://harriscrowne.net/     *      * @author David Knape     */    public class StageUpdateCommand extends BaseCommand implements ICommand {             override public function execute(notification:INotification):void        {            var stageProxy:StageProxy = facade.retrieveProxy( StageProxy.NAME ) as StageProxy;            stageProxy.update();        }    }}